{
  "name": "save-svg-as-png",
  "version": "1.4.17",
  "description": "Convert a browser SVG to PNG or dataUri",
  "type": "module",
  "main": "lib/index.umd.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.umd.cjs"
    }
  },
  "files": [
    "/lib"
  ],
  "scripts": {
    "build": "pnpm run /^build:.*/",
    "build:tsc": "tsc --build",
    "build:vite": "vite build",
    "dev": "vite serve demo/",
    "prepublish": "npm run build",
    "test": "node test.js",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/exupero/saveSvgAsPng"
  },
  "keywords": [
    "svg",
    "png",
    "graphics",
    "images"
  ],
  "author": "Eric Shull",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/exupero/saveSvgAsPng/issues"
  },
  "homepage": "https://github.com/exupero/saveSvgAsPng",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "eslint": "^8.57.0",
    "tape": "^5.7.5",
    "typescript": "^5.4.5",
    "vite": "^5.2.8"
  }
}
